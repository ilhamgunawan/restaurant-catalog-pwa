{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "eol-last": "off",
        "object-curly-newline": "off",
        "no-return-assign": "off",
        "no-param-reassign": "off",
        "max-len": "off",
        "linebreak-style": 0
    },
    "plugins": ["jasmine"]
}
